@model IEnumerable<SITW.Models.AssetsRecord>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <p>
        @Html.ActionLink("Create New", "Create")
    </p>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.UserId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.unitSn)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.gameSn)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.assets)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.type)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.inpdate)
            </th>
            <th></th>
        </tr>
    
    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.UserId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.unitSn)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.gameSn)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.assets)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.type)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.inpdate)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id=item.sn }) |
                @Html.ActionLink("Details", "Details", new { id=item.sn }) |
                @Html.ActionLink("Delete", "Delete", new { id=item.sn })
            </td>
        </tr>
    }
    
    </table>
</body>
</html>
