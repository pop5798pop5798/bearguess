@model SITW.Models.ViewModel.BetsByUserID
@{ 
    ViewBag.Title = "下注紀錄";
}

<div class="sportsmagazine-main-content">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                @Html.Action("_BetsByUserID", new { BetList = Model.BetList })             
            </div>
        </div>
    </div>
