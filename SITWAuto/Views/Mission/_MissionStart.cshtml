@model SITW.Models.ViewModel.MissionViewModel
@{
    Layout = null;
    int i = ViewBag.index ?? 0;
}
@using SITW.Helper



    
<div name="divMissionStart" class="form-horizontal">
    <h4>MissionStart</h4>
    <hr />
    @Html.ValidationSummary(true, "", new { @class = "text-danger" })

    <div class="form-group">
        @Html.LabelFor(model => model.MissionStartList[i].missionStartSn, htmlAttributes: new { @class = "control-label col-md-2" })
        <div class="col-md-10">
            @Html.MissionMissionStartSelect("MissionStartList[" + i + "].missionStartSn", Model.MissionStartList[i].missionStartSn)
            @Html.ValidationMessageFor(model => model.MissionStartList[i].missionStartSn, "", new { @class = "text-danger" })
        </div>
    </div>

    <div class="form-group">
        @Html.LabelFor(model => model.MissionStartList[i].Compare, htmlAttributes: new { @class = "control-label col-md-2" })
        <div class="col-md-10">
            @Html.MissionCompareSelect("MissionStartList[" + i + "].Compare", Model.MissionStartList[i].Compare)
            @Html.ValidationMessageFor(model => model.MissionStartList[i].Compare, "", new { @class = "text-danger" })
        </div>
    </div>

    <div class="form-group">
        @Html.LabelFor(model => model.MissionStartList[i].num, htmlAttributes: new { @class = "control-label col-md-2" })
        <div class="col-md-10">
            @Html.EditorFor(model => model.MissionStartList[i].num, new { htmlAttributes = new { @class = "form-control" } })
            @Html.ValidationMessageFor(model => model.MissionStartList[i].num, "", new { @class = "text-danger" })
        </div>
    </div>

    <div class="form-group">
        <div class="offset-md-2 col-md-10">
            <button type="button" class="btn btn-danger" name="btnDelete" onclick="deleteMissionStartEnd(this)">Delete</button>
        </div>
    </div>
</div>
