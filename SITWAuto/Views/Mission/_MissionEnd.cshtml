@model SITW.Models.ViewModel.MissionViewModel
@{
    Layout = null;
    int i = ViewBag.index ?? 0;
}
@using SITW.Helper

    
<div name="divMissionEnd" class="form-horizontal">
    <h4>MissionEnd</h4>
    <hr />
    @Html.ValidationSummary(true, "", new { @class = "text-danger" })

    <div class="form-group">
        @Html.LabelFor(model => model.MissionEndList[i].missionEndtSn, htmlAttributes: new { @class = "control-label col-md-2" })
        <div class="col-md-10">
            @Html.MissionMissionEndSelect("MissionEndList[" + i + "].missionEndtSn", Model.MissionEndList[i].missionEndtSn)
            @Html.ValidationMessageFor(model => model.MissionEndList[i].missionEndtSn, "", new { @class = "text-danger" })
        </div>
    </div>

    <div class="form-group">
        @Html.LabelFor(model => model.MissionEndList[i].Compare, htmlAttributes: new { @class = "control-label col-md-2" })
        <div class="col-md-10">
            @Html.MissionCompareSelect("MissionEndList["+ i + "].Compare", Model.MissionEndList[i].Compare)
            @Html.ValidationMessageFor(model => model.MissionEndList[i].Compare, "", new { @class = "text-danger" })
        </div>
    </div>

    <div class="form-group">
        @Html.LabelFor(model => model.MissionEndList[i].num, htmlAttributes: new { @class = "control-label col-md-2" })
        <div class="col-md-10">
            @Html.EditorFor(model => model.MissionEndList[i].num, new { htmlAttributes = new { @class = "form-control" } })
            @Html.ValidationMessageFor(model => model.MissionEndList[i].num, "", new { @class = "text-danger" })
        </div>
    </div>

    <div class="form-group">
        <div class="offset-md-2 col-md-10">
            <button type="button" class="btn btn-danger" name="btnDelete" onclick="deleteMissionStartEnd(this)">Delete</button>
        </div>
    </div>
</div>

